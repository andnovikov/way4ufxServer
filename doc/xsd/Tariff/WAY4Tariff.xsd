    <xs:complexType name="W4F_AgeingTariffRole">
        <xs:annotation>
            <xs:appinfo>Used to set up due normalisation parameters in account templates and, for instance, to calculate minimum payments during credit Product setup</xs:appinfo>
        </xs:annotation>
        <xs:complexContent>
            <xs:extension base="W4F_TariffGeneric">
                <xs:sequence>
                    <xs:sequence maxOccurs="1" minOccurs="1">
                        <xs:element maxOccurs="1" minOccurs="0" name="DuePeriod" type="W4D_Counter">
                            <xs:annotation>
                                <xs:appinfo>Used to determine the date of normalisation</xs:appinfo>
                            </xs:annotation>
                        </xs:element>
                        <xs:element maxOccurs="1" minOccurs="0" name="GracePeriod" type="W4D_Counter">
                            <xs:annotation>
                                <xs:appinfo>Used to specify the delay before generating a due normalisation accounting transfer</xs:appinfo>
                            </xs:annotation>
                        </xs:element>
                    </xs:sequence>
                    <xs:sequence maxOccurs="1" minOccurs="1">
                        <xs:element maxOccurs="1" minOccurs="0" name="MinRepayment" type="W4D_Amount">
                            <xs:annotation>
                                <xs:appinfo>If the account balance (or the account balance multiplied by the value of the Repayment Pcnt field) is less than the value specified in the Min Repayment field, funds are not transferred to the account specified in the Due Template field of the account template</xs:appinfo>
                            </xs:annotation>
                        </xs:element>
                        <xs:element maxOccurs="1" minOccurs="0" name="MinRqRepayment" type="W4D_Amount">
                            <xs:annotation>
                                <xs:appinfo>Minimum amount that must be transferred to the account specified in the Due Template field of the account template</xs:appinfo>
                            </xs:annotation>
                        </xs:element>
                        <xs:element maxOccurs="1" minOccurs="0" name="RepaymentPcnt" type="W4D_Amount">
                            <xs:annotation>
                                <xs:appinfo>Percentage that must be transferred to the account specified in the Due Template field of the account template</xs:appinfo>
                            </xs:annotation>
                        </xs:element>
                        <xs:element maxOccurs="1" minOccurs="0" name="Currency" type="W4D_Currency">
                            <xs:annotation>
                                <xs:appinfo>Deprecated and should not be used</xs:appinfo>
                            </xs:annotation>
                        </xs:element>
                    </xs:sequence>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <xs:complexType name="W4F_BillingSchemeTariffRole">
        <xs:annotation>
            <xs:appinfo>Used to set up billing cycle parameters.</xs:appinfo>
        </xs:annotation>
        <xs:complexContent>
            <xs:extension base="W4F_TariffGeneric">
                <xs:sequence>
                    <xs:element maxOccurs="1" minOccurs="0" name="BillingDay" type="W4D_Counter">
                        <xs:annotation>
                            <xs:appinfo>Shift of billing cycle end date</xs:appinfo>
                        </xs:annotation>
                    </xs:element>
                    <xs:element maxOccurs="1" minOccurs="0" name="MinBilling" type="W4D_Counter">
                        <xs:annotation>
                            <xs:appinfo>Minimum number of days in a billing cycle</xs:appinfo>
                        </xs:annotation>
                    </xs:element>
                    <xs:element maxOccurs="1" minOccurs="0" name="OVLTolerancePcnt" type="W4D_Amount"/>
                    <xs:element maxOccurs="1" minOccurs="0" name="OVLToleranceAmount" type="W4D_Amount"/>
                    <xs:element maxOccurs="1" minOccurs="0" name="AmountCurrency" type="W4D_Currency"/>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <xs:complexType name="W4F_GLNumberTariffRole">
        <xs:annotation>
            <xs:appinfo>Used during account template setup to specify GL account numbers.</xs:appinfo>
        </xs:annotation>
        <xs:complexContent>
            <xs:extension base="W4F_TariffGeneric">
                <xs:sequence>
                    <xs:element maxOccurs="1" minOccurs="0" name="GLNumber" type="W4D_LongCode">
                        <xs:annotation>
                            <xs:appinfo>GL account number used for synthetic accounting of analytic accounts generated using this template</xs:appinfo>
                        </xs:annotation>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <xs:complexType name="W4F_InstalmentFeeTariffRole">
        <xs:annotation>
            <xs:appinfo>Used to set up instalment loan fee parameters.</xs:appinfo>
        </xs:annotation>
        <xs:complexContent>
            <xs:extension base="W4F_TariffGeneric">
                <xs:sequence>
                    <xs:element maxOccurs="1" minOccurs="0" name="FeeMin" type="W4D_Amount">
                        <xs:annotation>
                            <xs:appinfo>Minimum fee amount</xs:appinfo>
                        </xs:annotation>
                    </xs:element>
                    <xs:element maxOccurs="1" minOccurs="0" name="FeeMax" type="W4D_Amount">
                        <xs:annotation>
                            <xs:appinfo>Maximum fee amount</xs:appinfo>
                        </xs:annotation>
                    </xs:element>
                    <xs:element maxOccurs="1" minOccurs="0" name="FeeBase" type="W4D_Amount">
                        <xs:annotation>
                            <xs:appinfo>Base fee amount</xs:appinfo>
                        </xs:annotation>
                    </xs:element>
                    <xs:element maxOccurs="1" minOccurs="0" name="FeeRatePcnt" type="W4D_Amount">
                        <xs:annotation>
                            <xs:appinfo>Fee percentage</xs:appinfo>
                        </xs:annotation>
                    </xs:element>
                    <xs:element maxOccurs="1" minOccurs="0" name="Currency" type="W4D_Currency">
                        <xs:annotation>
                            <xs:appinfo>Fee currency</xs:appinfo>
                        </xs:annotation>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <xs:complexType name="W4F_InstalmentSchemeTariffRole">
        <xs:annotation>
            <xs:appinfo>Used to set up parameters of instalment schemes that generate instalment plans.</xs:appinfo>
        </xs:annotation>
        <xs:complexContent>
            <xs:extension base="W4F_TariffGeneric">
                <xs:sequence>
                    <xs:sequence maxOccurs="1" minOccurs="1">
                        <xs:element maxOccurs="1" minOccurs="0" name="MinDealAmount" type="W4D_Amount">
                            <xs:annotation>
                                <xs:appinfo>Minimum amount of an initial transaction</xs:appinfo>
                            </xs:annotation>
                        </xs:element>
                        <xs:element maxOccurs="1" minOccurs="0" name="MaxDealAmount" type="W4D_Amount">
                            <xs:annotation>
                                <xs:appinfo>Maximum amount of an initial transaction</xs:appinfo>
                            </xs:annotation>
                        </xs:element>
                    </xs:sequence>
                    <xs:sequence maxOccurs="1" minOccurs="1">
                        <xs:sequence maxOccurs="1" minOccurs="1">
                            <xs:element maxOccurs="1" minOccurs="0" name="DueMode" type="W4D_InstSchemeTrfDueMode"/>
                            <xs:element maxOccurs="1" minOccurs="0" name="DuePeriod" type="W4D_Counter"/>
                        </xs:sequence>
                        <xs:sequence maxOccurs="1" minOccurs="1">
                            <xs:element maxOccurs="1" minOccurs="0" name="MinInstAmount" type="W4D_Amount">
                                <xs:annotation>
                                    <xs:appinfo>Minimum amount of an instalment in an instalment plan</xs:appinfo>
                                </xs:annotation>
                            </xs:element>
                            <xs:element maxOccurs="1" minOccurs="0" name="MaxInstAmount" type="W4D_Amount">
                                <xs:annotation>
                                    <xs:appinfo>Maximum amount of an instalment in an instalment plan</xs:appinfo>
                                </xs:annotation>
                            </xs:element>
                        </xs:sequence>
                        <xs:sequence maxOccurs="1" minOccurs="1">
                            <xs:element maxOccurs="1" minOccurs="0" name="TenorDefault" type="W4D_Counter">
                                <xs:annotation>
                                    <xs:appinfo>Default number of instalment months</xs:appinfo>
                                </xs:annotation>
                            </xs:element>
                            <xs:element maxOccurs="1" minOccurs="0" name="TenorMin" type="W4D_Counter">
                                <xs:annotation>
                                    <xs:appinfo>Minimum number of instalment months</xs:appinfo>
                                </xs:annotation>
                            </xs:element>
                            <xs:element maxOccurs="1" minOccurs="0" name="TenorMax" type="W4D_Counter">
                                <xs:annotation>
                                    <xs:appinfo>Maximum number of instalment months</xs:appinfo>
                                </xs:annotation>
                            </xs:element>
                        </xs:sequence>
                    </xs:sequence>
                    <xs:element maxOccurs="1" minOccurs="0" name="ShiftPlan" type="W4D_Amount"/>
                    <xs:element maxOccurs="1" minOccurs="0" name="Currency" type="W4D_Currency">
                        <xs:annotation>
                            <xs:appinfo>Currency in which limitations of an initial transaction amount and an instalment amount are specified</xs:appinfo>
                        </xs:annotation>
                    </xs:element>
                    <xs:element maxOccurs="1" minOccurs="0" name="SpecialParms" type="W4D_String">
                        <xs:annotation>
                            <xs:appinfo>Special parameters of the instalment scheme</xs:appinfo>
                        </xs:annotation>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <xs:complexType name="W4F_InterestTariffRole">
        <xs:annotation>
            <xs:appinfo>Used to set up interest rates in account templates.</xs:appinfo>
        </xs:annotation>
        <xs:complexContent>
            <xs:extension base="W4F_TariffGeneric">
                <xs:sequence>
                    <xs:element maxOccurs="1" minOccurs="0" name="RatePcnt" type="W4D_Amount">
                        <xs:annotation>
                            <xs:appinfo>Interest rate</xs:appinfo>
                        </xs:annotation>
                    </xs:element>
                    <xs:element maxOccurs="1" minOccurs="0" name="FeeRatePcnt" type="W4D_Amount">
                        <xs:annotation>
                            <xs:appinfo>Fee rate charged from the revenue in the form of deposit account interest</xs:appinfo>
                        </xs:annotation>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <xs:complexType name="W4F_InterestTaxTariffRole">
        <xs:annotation>
            <xs:appinfo>Used during setup of tariffs with the "Interest" role to specify interest rates of the financial gain tax for gain received in the form of deposit interest or saving on loan interest.</xs:appinfo>
        </xs:annotation>
        <xs:complexContent>
            <xs:extension base="W4F_TariffGeneric">
                <xs:sequence>
                    <xs:element maxOccurs="1" minOccurs="0" name="BaseRate" type="W4D_Amount">
                        <xs:annotation>
                            <xs:appinfo>Base rate for calculating the threshold value of the financial gain tax rate.</xs:appinfo>
                        </xs:annotation>
                    </xs:element>
                    <xs:element maxOccurs="1" minOccurs="0" name="TaxRate" type="W4D_Amount">
                        <xs:annotation>
                            <xs:appinfo>Tax rate</xs:appinfo>
                        </xs:annotation>
                    </xs:element>
                    <xs:element maxOccurs="1" minOccurs="0" name="BaseRateMultiplier" type="W4D_Amount">
                        <xs:annotation>
                            <xs:appinfo>Factor by which the base rate must be multiplied to calculate the threshold value over which the financial gain tax is charged</xs:appinfo>
                        </xs:annotation>
                    </xs:element>
                    <xs:element maxOccurs="1" minOccurs="0" name="BaseRateAddPoints" type="W4D_Amount">
                        <xs:annotation>
                            <xs:appinfo>Percentage that must be added to the base rate to calculate the threshold value over which the financial gain tax is charged</xs:appinfo>
                        </xs:annotation>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <xs:complexType name="W4F_ServiceLimitTariffRole">
        <xs:annotation>
            <xs:appinfo>Used to set up Services for calculating the maximum and minimum transaction amount, to specify account balance limitations (e.g. when a deposit Product with a floating interest rate is set up), and to specify limitations related to Events that open or close when the value of a specific balance type changes.</xs:appinfo>
        </xs:annotation>
        <xs:complexContent>
            <xs:extension base="W4F_TariffGeneric">
                <xs:sequence>
                    <xs:element maxOccurs="1" minOccurs="0" name="MaxAmount" type="W4D_Amount">
                        <xs:annotation>
                            <xs:appinfo>Maximum allowed transaction amount or account balance, or maximum threshold value of an Event related to balance type value changes. A zero value in the field means that there are no limitations</xs:appinfo>
                        </xs:annotation>
                    </xs:element>
                    <xs:element maxOccurs="1" minOccurs="0" name="MinAmount" type="W4D_Amount">
                        <xs:annotation>
                            <xs:appinfo>Minimum allowed transaction amount or account balance or minimum threshold value of an Event related to balance type value changes. A zero value in the field means that there are no limitations</xs:appinfo>
                        </xs:annotation>
                    </xs:element>
                    <xs:element maxOccurs="1" minOccurs="0" name="FloorLimit" type="W4D_Amount">
                        <xs:annotation>
                            <xs:appinfo>Maximum allowed transaction without authorization </xs:appinfo>
                        </xs:annotation>
                    </xs:element>
                    <xs:element maxOccurs="1" minOccurs="0" name="Currency" type="W4D_Currency">
                        <xs:annotation>
                            <xs:appinfo>Limiter currency</xs:appinfo>
                        </xs:annotation>
                    </xs:element>
                    <xs:element maxOccurs="1" minOccurs="0" name="ExpiryPeriod" type="W4D_Counter">
                        <xs:annotation>
                            <xs:appinfo>Expiry period</xs:appinfo>
                        </xs:annotation>
                    </xs:element>
                    <xs:element maxOccurs="1" minOccurs="0" name="ServicePriority" type="W4D_Counter"/>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <xs:complexType name="W4F_ServiceTariffRole">
        <xs:annotation>
            <xs:appinfo>Used to set up fee calculation rules in Services.</xs:appinfo>
        </xs:annotation>
        <xs:complexContent>
            <xs:extension base="W4F_TariffGeneric">
                <xs:sequence>
                    <xs:sequence maxOccurs="1" minOccurs="1">
                        <xs:element maxOccurs="1" minOccurs="0" name="FeeBase" type="W4D_Amount">
                            <xs:annotation>
                                <xs:appinfo>Additional fee whose charge is mandatory and not depending on a fee rate</xs:appinfo>
                            </xs:annotation>
                        </xs:element>
                        <xs:element maxOccurs="1" minOccurs="0" name="FeeMin" type="W4D_Amount">
                            <xs:annotation>
                                <xs:appinfo>Minimum fee amount that will be charged regardless of a transaction amount</xs:appinfo>
                            </xs:annotation>
                        </xs:element>
                        <xs:element maxOccurs="1" minOccurs="0" name="FeeMax" type="W4D_Amount">
                            <xs:annotation>
                                <xs:appinfo>Maximum transaction fee amount</xs:appinfo>
                            </xs:annotation>
                        </xs:element>
                    </xs:sequence>
                    <xs:sequence maxOccurs="1" minOccurs="1">
                        <xs:element maxOccurs="1" minOccurs="0" name="FeePcnt" type="W4D_Amount">
                            <xs:annotation>
                                <xs:appinfo>Fee rate</xs:appinfo>
                            </xs:annotation>
                        </xs:element>
                        <xs:element maxOccurs="1" minOccurs="0" name="IncreasePcnt" type="W4D_Amount">
                            <xs:annotation>
                                <xs:appinfo>Percentage added to authorisation and clearing amounts sent to the issuer</xs:appinfo>
                            </xs:annotation>
                        </xs:element>
                    </xs:sequence>
                    <xs:sequence maxOccurs="1" minOccurs="1">
                        <xs:element maxOccurs="1" minOccurs="0" name="Currency" type="W4D_Currency">
                            <xs:annotation>
                                <xs:appinfo>Fee currency</xs:appinfo>
                            </xs:annotation>
                        </xs:element>
                        <xs:element maxOccurs="1" minOccurs="0" name="FXRateType">
                            <xs:annotation>
                                <xs:appinfo>Deprecated and should not be used.</xs:appinfo>
                            </xs:annotation>
                            <xs:simpleType>
                                <xs:restriction base="xs:string">
                                    <xs:enumeration value="M"/>
                                    <xs:enumeration value="B"/>
                                </xs:restriction>
                            </xs:simpleType>
                        </xs:element>
                    </xs:sequence>
                    <xs:element maxOccurs="1" minOccurs="0" name="TariffPriority" type="W4D_Counter">
                        <xs:annotation>
                            <xs:appinfo>Deprecated and should not be used</xs:appinfo>
                        </xs:annotation>
                    </xs:element>
                    <xs:element maxOccurs="1" minOccurs="0" name="SpecialParms" type="W4D_String"/>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <xs:complexType name="W4F_ServiceValueDaysTariffRole">
        <xs:annotation>
            <xs:appinfo>Used to set up fields Value Days and Expiry Days in Services.</xs:appinfo>
        </xs:annotation>
        <xs:complexContent>
            <xs:extension base="W4F_TariffGeneric">
                <xs:sequence>
                    <xs:element maxOccurs="1" minOccurs="0" name="ValueDays" type="W4D_Counter">
                        <xs:annotation>
                            <xs:appinfo>Transaction payment time shift in days</xs:appinfo>
