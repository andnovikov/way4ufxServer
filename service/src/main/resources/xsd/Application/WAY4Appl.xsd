                </xs:element>
                <xs:element maxOccurs="1" minOccurs="0" name="NonfinanceLimit" type="W4C_CurrencyAmount">
                    <xs:annotation>
                        <xs:appinfo>Additional authorisation limit</xs:appinfo>
                    </xs:annotation>
                </xs:element>
            </xs:choice>
            <xs:element maxOccurs="1" minOccurs="0" name="ReasonDetails" type="W4D_String">
                <xs:annotation>
                    <xs:appinfo>System action reason details</xs:appinfo>
                </xs:annotation>
            </xs:element>
            <xs:element maxOccurs="1" minOccurs="0" name="ActivityPeriod" type="W4C_DateInterval">
                <xs:annotation>
                    <xs:appinfo>System action reason details</xs:appinfo>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="W4A_InstalmentLimit">
        <xs:sequence>
           <xs:element maxOccurs="1" minOccurs="1" name="LimitType" type="W4D_Code">
                <xs:annotation>
                    <xs:appinfo>Instalment limit type code</xs:appinfo>
                </xs:annotation>
            </xs:element>
            <xs:element maxOccurs="1" minOccurs="1" name="NewLimitValue">
                <xs:complexType>
                    <xs:sequence>
                        <xs:element maxOccurs="1" minOccurs="1" name="Amount" type="W4D_Amount">
                            <xs:annotation>
                                <xs:appinfo>Limit amount in contract currency</xs:appinfo>
                            </xs:annotation>
                        </xs:element>
                        <xs:element maxOccurs="1" minOccurs="0" name="Currency" type="W4D_Currency">
                            <xs:annotation>
                                <xs:appinfo>ISO-4217 currency code. Currently ignored. Reserved for future use</xs:appinfo>
                            </xs:annotation>
                        </xs:element>
                    </xs:sequence>
                </xs:complexType>
            </xs:element>
            <xs:element maxOccurs="1" minOccurs="1" name="ReasonDetails" type="W4D_String">
                <xs:annotation>
                    <xs:appinfo>System action reason details</xs:appinfo>
                </xs:annotation>
            </xs:element>
            <xs:element maxOccurs="1" minOccurs="0" name="ActivityPeriod" type="W4C_DateIntervalWithoutActivation">
                <xs:annotation>
                    <xs:appinfo>Reserved</xs:appinfo>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="W4A_QueEvent">
