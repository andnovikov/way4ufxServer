    <xs:complexType name="W4C_AddressRef">
        <xs:complexContent>
            <xs:extension base="W4C_Address">
                <xs:sequence>
                    <xs:element maxOccurs="1" minOccurs="0" name="RefActivityPeriod">
                        <xs:annotation>
                            <xs:appinfo>Reference an Address by its activity period. It is used to fill current Address Activity  dates, in case the Application is for changing the Address dates.</xs:appinfo>
                        </xs:annotation>
                        <xs:complexType>
                            <xs:complexContent>
                                <xs:restriction base="W4C_DateInterval">
                                    <xs:sequence>
                                        <xs:element maxOccurs="1" minOccurs="0" name="DateFrom" type="W4D_Date">
                                            <xs:annotation>
                                                <xs:appinfo>The initial date of the activity period</xs:appinfo>
                                            </xs:annotation>
                                        </xs:element>
                                        <xs:element maxOccurs="1" minOccurs="0" name="DateTo" type="W4D_Date">
                                            <xs:annotation>
                                                <xs:appinfo>The ending date of the activity period, inclusive</xs:appinfo>
                                            </xs:annotation>
                                        </xs:element>
                                    </xs:sequence>
                                </xs:restriction>
                            </xs:complexContent>
                        </xs:complexType>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <xs:complexType name="W4C_AddressRs">
        <xs:sequence>
            <xs:element maxOccurs="1" minOccurs="1" name="Address" type="W4C_Address">
                <xs:annotation>
                    <xs:appinfo>Address Data</xs:appinfo>
                </xs:annotation>
            </xs:element>
            <xs:element maxOccurs="1" minOccurs="0" name="Info">
                <xs:annotation>
                    <xs:appinfo>Address Additional Data</xs:appinfo>
                </xs:annotation>
                <xs:complexType>
                    <xs:sequence>
                        <xs:element maxOccurs="1" minOccurs="0" name="Source" type="W4D_Name">
                            <xs:annotation>
                                <xs:appinfo>Specifies from which object the address was obtained. Allowed values:
From Client Record
From Own Client
From Parent Client
From Own Contract
From Parent Contract

Also, value can have the hint "(Translated)" which means that original address info has been transliterated.
</xs:appinfo>
                            </xs:annotation>
                        </xs:element>
                        <xs:element maxOccurs="1" minOccurs="1" name="Status" type="W4D_ObjStatus">
                            <xs:annotation>
                                <xs:appinfo> Object activeness status. </xs:appinfo>
                            </xs:annotation>
                        </xs:element>
                        <xs:element maxOccurs="1" minOccurs="0" name="AmndInfo" ow:feature="Information_Address_AmndInfo" type="W4C_AmndInfo">
                            <xs:annotation>
                                <xs:appinfo>Amendment Information about the object</xs:appinfo>
                            </xs:annotation>
                        </xs:element>
                    </xs:sequence>
                </xs:complexType>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="W4C_AffiliationClientRs">
