                                                    <xs:appinfo>Set of Document Identifiers. Supported values: RRN, IRN,
 SRN, ARN, AuthCode, TRN, ReconsRN, ReceiptRN, DRN, PrevDRN, OrigDRN. Parm/ParmType is reserved for future use.</xs:appinfo>
                                                </xs:annotation>
                                            </xs:element>
                                        </xs:choice>
                                    </xs:restriction>
                                </xs:complexContent>
                            </xs:complexType>
                        </xs:element>
                        <xs:element maxOccurs="1" minOccurs="0" name="AddData" type="W4C_AddInfo">
                            <xs:annotation>
                                <xs:appinfo>Additional Response Data on the requested object.</xs:appinfo>
                            </xs:annotation>
                        </xs:element>
                    </xs:sequence>
                    <xs:element maxOccurs="1" minOccurs="1" name="Status" type="W4C_Status">
                        <xs:annotation>
