            <xs:element maxOccurs="1" minOccurs="0" name="AffiliationClient" ow:feature="Information_Contract_AffiliatedClient" type="W4C_Affiliation">
                <xs:annotation>
                    <xs:appinfo>Affiliated client data</xs:appinfo>
                </xs:annotation>
            </xs:element>
            <xs:element maxOccurs="1" minOccurs="0" name="Info">
                <xs:annotation>
                    <xs:appinfo>Address Additional Data</xs:appinfo>
                </xs:annotation>
                <xs:complexType>
                    <xs:sequence>
                        <xs:element maxOccurs="1" minOccurs="1" name="Status" type="W4D_ObjStatus">
                            <xs:annotation>
                                <xs:appinfo> Object activeness status. </xs:appinfo>
                            </xs:annotation>
                        </xs:element>
                        <xs:element maxOccurs="0" minOccurs="0" name="AmndInfo" type="W4C_AmndInfo">
                            <xs:annotation>
                                <xs:appinfo>Amendment Information about the object</xs:appinfo>
                            </xs:annotation>
                        </xs:element>
                    </xs:sequence>
                </xs:complexType>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="W4C_AffiliationClients">
        <xs:sequence>
            <xs:element maxOccurs="unbounded" minOccurs="1" name="AffiliationClientRs" type="W4C_AffiliationClientRs"/>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="W4C_AppInfo">
        <xs:sequence>
            <xs:element maxOccurs="1" minOccurs="0" name="InfoType" type="W4D_Code">
                <xs:annotation>
                    <xs:appinfo>Application Information type code.</xs:appinfo>
                </xs:annotation>
            </xs:element>
            <xs:element maxOccurs="1" minOccurs="0" name="Code" type="W4D_Code"/>
            <xs:element maxOccurs="1" minOccurs="0" name="GroupCode" type="W4D_Code">
                <xs:annotation>
                    <xs:appinfo>Application Information Group Code</xs:appinfo>
                </xs:annotation>
            </xs:element>
            <xs:element maxOccurs="1" minOccurs="0" name="AddInfo01" type="W4D_Name">
                <xs:annotation>
                    <xs:appinfo>Additional information, line 1.</xs:appinfo>
                </xs:annotation>
            </xs:element>
            <xs:element maxOccurs="1" minOccurs="0" name="AddInfo02" type="W4D_Name">
                <xs:annotation>
                    <xs:appinfo>Additional information, line 2.</xs:appinfo>
                </xs:annotation>
            </xs:element>
            <xs:element maxOccurs="1" minOccurs="0" name="AddInfo03" type="W4D_Name">
                <xs:annotation>
                    <xs:appinfo>Additional information, line 3.</xs:appinfo>
                </xs:annotation>
