                                his contract,
                                "Relation" - link to a related contract,
                                "ClientAddress" - client address,
                                "ContractAddress" - contract address,
                                "ClientAddInfo" - record containing
                                additional client information
                                "ContractAddInfo"- record containing
                                additional contract information,
                                "ClientAffiliation" - Affiliation Client for a Client,
                                "ContractAffiliation" - Affiliation Client for a Contract,
                                "CustomerClassifier" - Customer Classifier,
                                "ClientClassifier" - Client Classifier,
                                "ContractClassifier" - Contract Classifier,
                                "ContractParameter" - Contract Parameter,
                                "Client Parameter" - Client Parameter,
                                "CustomerIdentifier" - Customer Identifier,
                                "Tariff" - Tariff for Contract,
                                "Limit" - contract credit limit,
                                "InstalmentLimit" - contract instalment limit,
                                "Event" - contract event,
                                "Card" - plastic card,
                                "Status" - contract status,
                                "Order" - standing payment order,
                                "Usage" - usage limiter
                                "Preferred" - contractor,
                                "AuthMethod" - deprecated,
                                "ClientAuthMethod" - authentication method for a Client,
                                "ContractAuthMethod" - authentication method for a Contract,
                                "CustomerAuthMethod" - authentication method for a Customer,
                                "Application" - application record
