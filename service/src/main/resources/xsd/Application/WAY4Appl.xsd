    <xs:complexType name="W4A_AccessInfo">
        <xs:sequence>
            <xs:element maxOccurs="1" minOccurs="1" name="PinAttempts" type="W4D_Counter">
                <xs:annotation>
                    <xs:appinfo>Counter of successive PIN tries</xs:appinfo>
                </xs:annotation>
            </xs:element>
            <xs:element maxOccurs="1" minOccurs="1" name="MaxPinAttempts" type="W4D_Counter">
                <xs:annotation>
                    <xs:appinfo>Maximum number of PIN tries</xs:appinfo>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="W4A_CardProduction">
        <xs:sequence>
            <xs:element maxOccurs="1" minOccurs="0" name="DeliveryDprt" type="W4D_OrderDprt">
                <xs:annotation>
                    <xs:appinfo>Number of bank department the
issued plastic will be delivered to</xs:appinfo>
                </xs:annotation>
            </xs:element>
            <xs:element maxOccurs="1" minOccurs="0" name="ProductionType" type="W4D_Code">
                <xs:annotation>
                    <xs:appinfo>Card production type:
"Plastic " - re-produce a plastic, a new plastic
will be issued with new CVV1/CVV2 and PVV
re-calculated for the old PIN value,
"PlasticAndPIN " - produce a new plastic with
new CVV1/CVV2 and new PIN,
"CVV" - produce a new plastic with new
CVV1/CVV2 without PIN/PVV,
"ReorderPIN" - reorder PIN,
"ReplaceAddParms" - replace additional parameters
</xs:appinfo>
                </xs:annotation>
            </xs:element>
            <xs:element maxOccurs="1" minOccurs="0" name="ProductionEvent" type="W4D_CardProdEvent">
                <xs:annotation>
                    <xs:appinfo>Reason for issuing/reissuing the plastic card.
Codes are defined in the "Full \
Configuration Setup \ Transaction Types \
Production Events" table.</xs:appinfo>
                </xs:annotation>
            </xs:element>
            <xs:element maxOccurs="1" minOccurs="0" name="ChipScheme" type="W4D_Name">
                <xs:annotation>
                    <xs:appinfo>Chip card Risk Scheme. Used to
determine EMV card offline limits.</xs:appinfo>
                </xs:annotation>
            </xs:element>
            <xs:element maxOccurs="1" minOccurs="0" name="CardNumber" type="W4D_ContractNumber">
                <xs:annotation>
                    <xs:appinfo>Card contract number;
                        Not supported in R1 applications
                    </xs:appinfo>
                </xs:annotation>
            </xs:element>
            <xs:element maxOccurs="1" minOccurs="0" name="CardExpiry" type="W4D_CardExpire">
                <xs:annotation>
                    <xs:appinfo>Plastic expiration date</xs:appinfo>
                </xs:annotation>
            </xs:element>
            <xs:element maxOccurs="1" minOccurs="0" name="CommentText" type="W4D_Comment">
                <xs:annotation>
                    <xs:appinfo>Comments for plastic production</xs:appinfo>
                </xs:annotation>
            </xs:element>
            <xs:element maxOccurs="1" minOccurs="0" name="SequenceNumber" type="W4D_Name">
                <xs:annotation>
                    <xs:appinfo>Card sequence number</xs:appinfo>
                </xs:annotation>
            </xs:element>
            <xs:element maxOccurs="1" minOccurs="0" name="SecurityParms" type="W4C_AddInfo">
                <xs:annotation>
                    <xs:appinfo>Additional plastic information</xs:appinfo>
                </xs:annotation>
            </xs:element>
            <xs:element maxOccurs="1" minOccurs="0" name="AddData" type="W4C_ParmSet">
                <xs:annotation>
                    <xs:documentation>Additional Data. The following ParmCodes are supported on import:
'ErasePIN' with value 'Yes' - request to erase pin for card.
Parm/ParmType is reserved for future use.</xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="W4A_CardRelation">
        <xs:sequence>
            <xs:element maxOccurs="1" minOccurs="1" name="IsActive" type="W4D_YesNo">
                <xs:annotation>
                    <xs:appinfo>Deactivating additional contract relations.
"No" deactivates the relation.</xs:appinfo>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="W4A_ChangeStatus">
        <xs:sequence>
            <xs:element maxOccurs="1" minOccurs="1" name="StatusCode" type="W4D_Code">
                <xs:annotation>
                    <xs:appinfo>Contract status. Codes are defined in the
"Full \ Configuration Setup \
Contract Types \ Contract Statuses" table.</xs:appinfo>
                </xs:annotation>
            </xs:element>
            <xs:element maxOccurs="1" minOccurs="1" name="StatusComment" type="W4D_Comment">
                <xs:annotation>
                    <xs:appinfo>Comments on contract status</xs:appinfo>
                </xs:annotation>
            </xs:element>
            <xs:element maxOccurs="1" minOccurs="0" name="CloseDate" type="W4D_Date">
                <xs:annotation>
                    <xs:appinfo>Contract closing date. This element
is used only when a contract is closed.</xs:appinfo>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="W4A_CreditLimit">
        <xs:sequence>
            <xs:choice maxOccurs="1" minOccurs="0">
