<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:ow="http://www.openwaygroup.com/xmlns/ufxtypes" attributeFormDefault="unqualified" elementFormDefault="unqualified" ow:feature="CustomerProfile">
    <xs:include schemaLocation="../WAY4Types.xsd"/>
    <xs:include schemaLocation="../Application/WAY4Appl.xsd"/>
    <xs:simpleType name="W4D_BodyType">
        <xs:restriction base="xs:string">
            <xs:enumeration value="Xml"/>
            <xs:enumeration value="Text"/>
            <xs:enumeration value="Attachment"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="W4D_InOut">
        <xs:restriction base="xs:string">
            <xs:enumeration value="Incoming"/>
            <xs:enumeration value="Outgoing"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="W4P_Direction">
        <xs:restriction base="xs:string">
            <xs:enumeration value="None"/>
            <xs:enumeration value="In"/>
            <xs:enumeration value="InOut"/>
            <xs:enumeration value="Out"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="W4P_PhaseType">
        <xs:restriction base="xs:string">
            <xs:enumeration value="Input"/>
            <xs:enumeration value="Confirm"/>
            <xs:enumeration value="EndEMVTransaction"/>
            <xs:enumeration value="CashIn"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="W4P_UseType">
        <xs:restriction base="xs:string">
            <xs:enumeration value="Main"/>
            <xs:enumeration value="Attachment"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:complexType name="W4C_Attachment">
        <xs:sequence>
            <xs:element maxOccurs="1" minOccurs="1" name="Id" type="W4D_Counter"/>
            <xs:element maxOccurs="1" minOccurs="0" name="Name" type="W4D_Code"/>
            <xs:element maxOccurs="1" minOccurs="0" name="MIMEType" type="W4D_Code">
                <xs:annotation>
                    <xs:documentation>MIME content type</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element maxOccurs="1" minOccurs="0" name="Charset" type="W4D_Code">
                <xs:annotation>
                    <xs:documentation>MIME content encoding</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element maxOccurs="1" minOccurs="0" name="UseType" type="W4P_UseType"/>
            <xs:element maxOccurs="1" minOccurs="0" name="FileDtls">
                <xs:annotation>
                    <xs:documentation>Extra properties. Parm/ParmType is reserved for future use</xs:documentation>
                </xs:annotation>
                <xs:complexType>
                    <xs:complexContent>
                        <xs:extension base="W4C_ParmSetFree">
                            <xs:sequence>
                                <xs:element maxOccurs="1" minOccurs="0" name="FileLink" type="W4P_FileLink"/>
                            </xs:sequence>
                        </xs:extension>
                    </xs:complexContent>
                </xs:complexType>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="W4P_Address">
        <xs:sequence>
            <xs:element maxOccurs="1" minOccurs="0" name="DisplayName" type="W4D_Name"/>
            <xs:choice maxOccurs="1" minOccurs="0">
                <xs:element maxOccurs="1" minOccurs="1" name="Code" type="W4D_Code"/>
                <xs:element maxOccurs="1" minOccurs="1" name="EMail" type="W4D_Code"/>
            </xs:choice>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="W4P_AffiliatedSystem">
        <xs:sequence>
            <xs:element maxOccurs="1" minOccurs="1" name="Code" type="W4D_Code">
                <xs:annotation>
                    <xs:appinfo>Affiliated System Code is a unique identifier of a Client Application. It can be used for a request routing</xs:appinfo>
                </xs:annotation>
            </xs:element>
            <xs:element maxOccurs="1" minOccurs="0" name="DisplayName" type="W4D_Name">
                <xs:annotation>
                    <xs:appinfo>Human readable name of an Affiliated System</xs:appinfo>
                </xs:annotation>
            </xs:element>
            <xs:element maxOccurs="1" minOccurs="0" name="ExtObjectFor">
                <xs:annotation>
                    <xs:appinfo>External Object identifier. An object such as a Client can have a special identifier in the Affiliated System different from its internal identifiers. ExtObjectFor is used to present clients' external identifiers so that the Affiliated System recognises them.</xs:appinfo>
                </xs:annotation>
                <xs:complexType>
                    <xs:complexContent>
                        <xs:restriction base="W4C_ObjectFor">
                            <xs:choice>
                                <xs:element maxOccurs="1" minOccurs="1" name="ClientIDT" type="W4C_ClientIDT">
                                    <xs:annotation>
                                        <xs:appinfo>Client's indentifying information. Indicated if the
request object is identified by client.</xs:appinfo>
                                    </xs:annotation>
                                </xs:element>
                            </xs:choice>
                        </xs:restriction>
                    </xs:complexContent>
                </xs:complexType>
            </xs:element>
            <xs:element maxOccurs="1" minOccurs="0" name="ExtFilters">
                <xs:complexType>
                    <xs:sequence>
                        <xs:element maxOccurs="unbounded" minOccurs="1" name="Filter" type="W4C_Filter"/>
                    </xs:sequence>
                </xs:complexType>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="W4P_AffiliatedSystems">
        <xs:sequence>
            <xs:element maxOccurs="unbounded" minOccurs="1" name="AffiliatedSystem" type="W4P_AffiliatedSystem">
                <xs:annotation>
                    <xs:appinfo>AffiliatedSystem is a Software Application which can provide some service, such as Payment processing or Information presentation. The contained data can be used for request routing</xs:appinfo>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="W4P_AssociatedObject">
        <xs:sequence>
            <xs:element maxOccurs="1" minOccurs="1" name="AssociatedObjectIDT" type="W4P_AssociatedObjectIDT"/>
            <xs:element maxOccurs="0" minOccurs="0" name="AssociatedObjectInfo" type="W4P_Extra">
                <xs:annotation>
                    <xs:appinfo>Service Additional Information.</xs:appinfo>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="W4P_AssociatedObjectIDT">
        <xs:sequence>
            <xs:element maxOccurs="1" minOccurs="1" name="Code" type="W4D_Code">
                <xs:annotation>
                    <xs:appinfo>Service Identifier.</xs:appinfo>
                </xs:annotation>
            </xs:element>
            <xs:element maxOccurs="1" minOccurs="0" name="Type" type="W4D_Code">
                <xs:annotation>
                    <xs:appinfo>Service type</xs:appinfo>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="W4P_Attachment">
        <xs:complexContent>
            <xs:extension base="W4C_Attachment"/>
        </xs:complexContent>
    </xs:complexType>
    <xs:complexType name="W4P_Calculation">
        <xs:sequence>
            <xs:element maxOccurs="1" minOccurs="1" name="Code" type="W4D_Code"/>
            <xs:element maxOccurs="1" minOccurs="0" name="Script" type="W4D_Name"/>
            <xs:element maxOccurs="1" minOccurs="0" name="Parms" type="W4C_ParmSet">
                <xs:annotation>
                    <xs:appinfo>Parm/ParmType is reserved for future use</xs:appinfo>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="W4P_Condition">
        <xs:sequence>
            <xs:element maxOccurs="1" minOccurs="1" name="Code" type="W4D_Code"/>
            <xs:element maxOccurs="1" minOccurs="0" name="Script" type="W4D_Name"/>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="W4P_ConditionIDT">
        <xs:complexContent>
            <xs:restriction base="W4P_Condition">
                <xs:sequence>
                    <xs:element maxOccurs="1" minOccurs="1" name="Code" type="W4D_Code"/>
                </xs:sequence>
            </xs:restriction>
        </xs:complexContent>
    </xs:complexType>
    <xs:complexType name="W4P_Conditions">
        <xs:sequence>
            <xs:element maxOccurs="unbounded" minOccurs="1" name="Condition" type="W4P_Condition"/>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="W4P_CustomerProfile">
        <xs:sequence>
            <xs:sequence maxOccurs="1" minOccurs="1">
                <xs:element maxOccurs="1" minOccurs="1" name="RegNumber" type="W4D_Name">
                    <xs:annotation>
                        <xs:appinfo>Registration Number</xs:appinfo>
                    </xs:annotation>
                </xs:element>
                <xs:element maxOccurs="1" minOccurs="0" name="LocalDt" type="W4D_DateTime"/>
                <xs:sequence maxOccurs="1" minOccurs="1">
                    <xs:element maxOccurs="1" minOccurs="1" name="Requestor" type="W4C_Contractor">
                        <xs:annotation>
                            <xs:appinfo>Request initiator. Participates in online requests as a person who requests that the request should be performed. Secret values and privileges to perform operations with ObjectFor (e.g. Card) are checked for this participant.</xs:appinfo>
                        </xs:annotation>
                    </xs:element>
                    <xs:element maxOccurs="1" minOccurs="1" name="Source" type="W4C_Contractor">
                        <xs:annotation>
                            <xs:appinfo>Request sender. Device from which a request is received (e.g. an ATM).</xs:appinfo>
                        </xs:annotation>
                    </xs:element>
                    <xs:element maxOccurs="1" minOccurs="0" name="ValidationDtls" type="W4C_ParmSet">
                        <xs:annotation>
                            <xs:appinfo>Request validation parameters. Parm/ParmType is reserved for future use</xs:appinfo>
                        </xs:annotation>
                    </xs:element>
                </xs:sequence>
            </xs:sequence>
            <xs:sequence maxOccurs="1" minOccurs="1">
                <xs:element maxOccurs="1" minOccurs="1" name="ObjectType">
                    <xs:annotation>
                        <xs:appinfo>Object with which operation should be executed: "Menu" - menu</xs:appinfo>
                    </xs:annotation>
                    <xs:simpleType>
                        <xs:restriction base="xs:string">
                            <xs:enumeration value="AffiliatedSystem"/>
                            <xs:enumeration value="Operation"/>
                            <xs:enumeration value="Template"/>
                            <xs:enumeration value="Menu"/>
                            <xs:enumeration value="HandBook"/>
                            <xs:enumeration value="Service"/>
                            <xs:enumeration value="OperationContext"/>
                            <xs:enumeration value="Letter"/>
                            <xs:enumeration value="FavoriteMenu"/>
                        </xs:restriction>
                    </xs:simpleType>
                </xs:element>
                <xs:element maxOccurs="1" minOccurs="1" name="ActionType">
                    <xs:annotation>
                        <xs:appinfo>Action Type for an Object (see comments for the ObjectType element for supported type information). Actions applied to the object: "Create"  create an object; Update"  update the object; "Remove"  remove the object, "Inquiry"  information Inquiry; "Execute"  execute the action.</xs:appinfo>
                    </xs:annotation>
                    <xs:simpleType>
                        <xs:restriction base="xs:string">
                            <xs:enumeration value="Add"/>
                            <xs:enumeration value="Inquiry"/>
                            <xs:enumeration value="Execute"/>
                            <xs:enumeration value="Create"/>
                            <xs:enumeration value="Update"/>
                            <xs:enumeration value="Remove"/>
                            <xs:enumeration value="Activate"/>
                            <xs:enumeration value="Deactivate"/>
                            <xs:enumeration value="Iterate"/>
                            <xs:enumeration value="Refresh"/>
                            <xs:enumeration value="Proceed"/>
                        </xs:restriction>
                    </xs:simpleType>
                </xs:element>
                <xs:element maxOccurs="1" minOccurs="0" name="ResultDtls" type="W4C_ResultDtls">
                    <xs:annotation>
                        <xs:appinfo>Result parameters specifying data necessary in the response in elements array Parm; and Filter information in Elements array Filter. 
Parm. Supported values: "RequestedLanguage"  language into which response data should be translated. See section "ResultDtls Codes" in the UFX specification for full information. Parm/ParmCode is used for Code, and Parm/Value is used for a Value corresponding to the Code.
</xs:appinfo>
                    </xs:annotation>
                </xs:element>
            </xs:sequence>
            <xs:sequence maxOccurs="1" minOccurs="1">
                <xs:element maxOccurs="1" minOccurs="0" name="ObjectFor">
                    <xs:annotation>
                        <xs:appinfo>Object Identifier. It contains a reference to an Object for which the action is requested. Its type may differ from the type defined in the ObjectType element.</xs:appinfo>
                    </xs:annotation>
                    <xs:complexType>
                        <xs:choice>
                            <xs:element maxOccurs="1" minOccurs="1" name="MenuIDT">
                                <xs:annotation>
                                    <xs:appinfo>Menu Identifier. It contains a reference to a Menu. See the DataRs/Menu definition for more details</xs:appinfo>
                                </xs:annotation>
                                <xs:complexType>
                                    <xs:complexContent>
                                        <xs:restriction base="W4P_Menu">
                                            <xs:sequence>
                                                <xs:sequence maxOccurs="1" minOccurs="1">
                                                    <xs:element maxOccurs="1" minOccurs="1" name="Code" type="W4D_Code">
                                                        <xs:annotation>
                                                            <xs:appinfo>Menu Code. It is a unique identifier of a Menu</xs:appinfo>
                                                        </xs:annotation>
                                                    </xs:element>
                                                </xs:sequence>
                                            </xs:sequence>
                                        </xs:restriction>
                                    </xs:complexContent>
                                </xs:complexType>
                            </xs:element>
                            <xs:element maxOccurs="1" minOccurs="1" name="ServiceIDT" type="W4P_ServiceIDT">
                                <xs:annotation>
                                    <xs:appinfo>Service Identifier. It contains a reference to a Service. See the DataRs/Service definition for more details.</xs:appinfo>
                                </xs:annotation>
                            </xs:element>
                            <xs:element maxOccurs="1" minOccurs="1" name="HandBookIDT">
                                <xs:annotation>
                                    <xs:appinfo>HandBook Identifier. It contains a reference to a HandBook See definition to DataRs/HandBook definition for more details.</xs:appinfo>
                                </xs:annotation>
                                <xs:complexType>
                                    <xs:complexContent>
                                        <xs:extension base="W4P_HandBookIDT">
                                            <xs:sequence>
                                                <xs:element maxOccurs="1" minOccurs="0" name="Filter">
                                                    <xs:annotation>
                                                        <xs:appinfo>A Filter defines a Property whose data can be used for HandBook filtration.</xs:appinfo>
                                                    </xs:annotation>
                                                    <xs:complexType>
                                                        <xs:sequence>
                                                            <xs:element maxOccurs="1" minOccurs="1" name="Properties" type="W4P_Properties"/>
                                                        </xs:sequence>
                                                    </xs:complexType>
                                                </xs:element>
                                            </xs:sequence>
                                        </xs:extension>
                                    </xs:complexContent>
                                </xs:complexType>
                            </xs:element>
                            <xs:element maxOccurs="1" minOccurs="1" name="OperationIDT" type="W4P_OperationIDT">
                                <xs:annotation>
                                    <xs:appinfo>Operation Identifier. It contains a reference to an Operation. See definition to DataRs/Operations/Operation definition for more details.</xs:appinfo>
                                </xs:annotation>
                            </xs:element>
                            <xs:element maxOccurs="1" minOccurs="1" name="TemplateIDT" type="W4P_TemplateIDT">
                                <xs:annotation>
                                    <xs:appinfo>Template Identifier. It contains a reference to a Template. See definition to DataRs/Templates/Template definition for more details.</xs:appinfo>
                                </xs:annotation>
                            </xs:element>
                            <xs:element maxOccurs="1" minOccurs="1" name="Letters" type="W4P_LettersIDT"/>
                            <xs:element maxOccurs="1" minOccurs="1" name="OperationContext" type="W4P_OperationContext"/>
                            <xs:element maxOccurs="1" minOccurs="1" name="MenuItems" type="W4P_MenuItemsFavorites"/>
                        </xs:choice>
                    </xs:complexType>
                </xs:element>
                <xs:choice maxOccurs="1" minOccurs="1">
                    <xs:element maxOccurs="1" minOccurs="0" name="Data">
                        <xs:annotation>
                            <xs:appinfo>Data to be processed corresponding to a request for an Object whose type is defined in the ObjectType element</xs:appinfo>
                        </xs:annotation>
                        <xs:complexType>
                            <xs:choice>
                                <xs:element maxOccurs="1" minOccurs="1" name="Operation">
                                    <xs:annotation>
                                        <xs:appinfo>Operation Data to be processed corresponding to a request</xs:appinfo>
                                    </xs:annotation>
                                    <xs:complexType>
                                        <xs:complexContent>
                                            <xs:restriction base="W4P_Operation">
                                                <xs:sequence>
                                                    <xs:element maxOccurs="1" minOccurs="1" name="OperationIDT" type="W4P_OperationIDT">
                                                        <xs:annotation>
                                                            <xs:appinfo>Operation Identifier. It contains identification information for an Operation.</xs:appinfo>
                                                        </xs:annotation>
                                                    </xs:element>
                                                    <xs:element maxOccurs="1" minOccurs="0" name="LocalDt" type="W4D_DateTime">
                                                        <xs:annotation>
                                                            <xs:appinfo>Request Local Date (transaction date).</xs:appinfo>
                                                        </xs:annotation>
                                                    </xs:element>
                                                    <xs:element maxOccurs="1" minOccurs="0" name="Properties" type="W4P_Properties">
                                                        <xs:annotation>
                                                            <xs:appinfo>Property Set. It contains Operation data as a set of Properties</xs:appinfo>
                                                        </xs:annotation>
                                                    </xs:element>
                                                    <xs:sequence maxOccurs="1" minOccurs="1">
                                                        <xs:element maxOccurs="1" minOccurs="0" name="Status" type="W4P_Status"/>
                                                        <xs:element maxOccurs="1" minOccurs="0" name="TemplateIDT" type="W4P_TemplateIDTRst">
                                                            <xs:annotation>
                                                                <xs:appinfo>Template Identifier. It contains identification information for an Template</xs:appinfo>
                                                            </xs:annotation>
                                                        </xs:element>
                                                    </xs:sequence>
                                                    <xs:choice maxOccurs="1" minOccurs="0">
                                                        <xs:sequence maxOccurs="1" minOccurs="1">
                                                            <xs:element maxOccurs="1" minOccurs="1" name="PostingDate" type="W4D_Date">
                                                                <xs:annotation>
                                                                    <xs:appinfo>Operation Posting date. If it is present in a request, it means the requested Operation posting date. In responses, it means the date when the Operation was posted.</xs:appinfo>
                                                                </xs:annotation>
                                                            </xs:element>
                                                        </xs:sequence>
                                                    </xs:choice>
                                                </xs:sequence>
                                            </xs:restriction>
                                        </xs:complexContent>
                                    </xs:complexType>
                                </xs:element>
                                <xs:element maxOccurs="1" minOccurs="1" name="Template" type="W4P_TemplateData">
                                    <xs:annotation>
                                        <xs:appinfo>Template Data to be processed corresponding to a request</xs:appinfo>
                                    </xs:annotation>
                                </xs:element>
                                <xs:element maxOccurs="1" minOccurs="1" name="Letter" type="W4P_Letter"/>
                                <xs:element maxOccurs="1" minOccurs="1" name="PreviousContext" type="W4P_PreviousContext"/>
                            </xs:choice>
                        </xs:complexType>
                    </xs:element>
                    <xs:sequence maxOccurs="1" minOccurs="1">
                        <xs:element maxOccurs="1" minOccurs="0" name="DataRs">
                            <xs:annotation>
                                <xs:appinfo>Response Data. Aggregate contains data specific for a response to a request.</xs:appinfo>
                            </xs:annotation>
                            <xs:complexType>
                                <xs:choice>
                                    <xs:element maxOccurs="1" minOccurs="1" name="Menu" type="W4P_Menu">
                                        <xs:annotation>
                                            <xs:appinfo>A Menu is a hierarchical structure used to allow a terminal to help a Customer to select the necessary service. A Menu contains content data. It does not contain formatting instructions.</xs:appinfo>
                                        </xs:annotation>
                                    </xs:element>
                                    <xs:element maxOccurs="1" minOccurs="1" name="Service" type="W4P_Service">
                                        <xs:annotation>
                                            <xs:appinfo>The structure is used to return defined Service information. A Service defines a set of Properties to be filled in to perform an Operation (for details, see comment for DataRs/Operation/Operation). The Code is the same as in Menu/ServiceCode.</xs:appinfo>
                                        </xs:annotation>
                                    </xs:element>
                                    <xs:element maxOccurs="1" minOccurs="1" name="HandBook" type="W4P_HandBook">
                                        <xs:annotation>
                                            <xs:appinfo>A HandBook is a dictionary. It can present different types of static information about existing objects, such as Countries, Banks, or Kindergarten</xs:appinfo>
                                        </xs:annotation>
                                    </xs:element>
                                    <xs:element maxOccurs="1" minOccurs="1" name="Operations" type="W4P_Operations">
                                        <xs:annotation>
                                            <xs:appinfo>Set of Operations</xs:appinfo>
                                        </xs:annotation>
                                    </xs:element>
                                    <xs:element maxOccurs="1" minOccurs="1" name="Templates" type="W4P_Templates">
                                        <xs:annotation>
                                            <xs:appinfo>Set of Templates</xs:appinfo>
                                        </xs:annotation>
                                    </xs:element>
                                    <xs:element maxOccurs="1" minOccurs="1" name="AffiliatedSystems" type="W4P_AffiliatedSystems">
                                        <xs:annotation>
                                            <xs:appinfo>Set of Affiliated Systems</xs:appinfo>
                                        </xs:annotation>
                                    </xs:element>
                                    <xs:element maxOccurs="1" minOccurs="1" name="Letters" type="W4P_Letters"/>
                                    <xs:element maxOccurs="1" minOccurs="1" name="OperationContext" type="W4P_OperationContextRs"/>
                                </xs:choice>
                            </xs:complexType>
                        </xs:element>
                        <xs:element maxOccurs="1" minOccurs="1" name="Status" type="W4P_Status">
                            <xs:annotation>
                                <xs:appinfo> Loading status</xs:appinfo>
                            </xs:annotation>
                        </xs:element>
                    </xs:sequence>
                </xs:choice>
                <xs:element maxOccurs="1" minOccurs="0" name="AddData" type="W4C_AddInfo">
                    <xs:annotation>
                        <xs:appinfo> Additional Data on the requested object.</xs:appinfo>
                    </xs:annotation>
                </xs:element>
            </xs:sequence>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="W4P_Dependencies">
        <xs:sequence>
            <xs:element maxOccurs="unbounded" minOccurs="0" name="Dependency" type="W4P_Dependency"/>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="W4P_Dependency">
        <xs:sequence>
            <xs:sequence maxOccurs="1" minOccurs="1">
                <xs:element maxOccurs="1" minOccurs="1" name="Type">
                    <xs:simpleType>
                        <xs:restriction base="xs:string">
                            <xs:enumeration value="Calculation"/>
                            <xs:enumeration value="Filter"/>
                            <xs:enumeration value="HandBook"/>
                            <xs:enumeration value="ResultDtls"/>
                            <xs:enumeration value="EditMask"/>
                        </xs:restriction>
                    </xs:simpleType>
                </xs:element>
                <xs:element maxOccurs="1" minOccurs="0" name="Code" type="W4D_Code"/>
            </xs:sequence>
            <xs:sequence maxOccurs="1" minOccurs="1">
                <xs:element maxOccurs="unbounded" minOccurs="0" name="Property">
                    <xs:complexType>
                        <xs:complexContent>
                            <xs:extension base="W4P_PropertyCode">
                                <xs:sequence>
                                    <xs:element maxOccurs="1" minOccurs="0" name="ParmCode" type="W4D_Code"/>
                                </xs:sequence>
                            </xs:extension>
                        </xs:complexContent>
                    </xs:complexType>
                </xs:element>
            </xs:sequence>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="W4P_DisplayComments">
        <xs:sequence>
            <xs:element maxOccurs="unbounded" minOccurs="1" name="DisplayComment" type="W4D_Name"/>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="W4P_Extra">
        <xs:choice>
            <xs:element maxOccurs="1" minOccurs="0" name="AddData" type="W4C_ParmSet">
                <xs:annotation>
                    <xs:appinfo>Additional Information. Parm/ParmType is reserved for future use</xs:appinfo>
                </xs:annotation>
            </xs:element>
            <xs:element maxOccurs="1" minOccurs="0" name="AddInfo" type="W4C_AddInfo">
                <xs:annotation>
                    <xs:appinfo>Additional Information</xs:appinfo>
                </xs:annotation>
            </xs:element>
        </xs:choice>
    </xs:complexType>
    <xs:complexType name="W4P_FileLink">
        <xs:sequence>
            <xs:element maxOccurs="1" minOccurs="0" name="LinkType">
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <xs:enumeration value="Url"/>
                        <xs:enumeration value="ClientSide"/>
                        <xs:enumeration value="FileServer"/>
                    </xs:restriction>
